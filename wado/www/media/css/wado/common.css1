.pb20{bottom:20px;}
.pa2{padding:2px;}

.viewer img{height:100%;}
/*.wrapper{display:table-cell;}*/
.half img{height:auto;}
#vFull{overflow-y:auto;}
#vReport{overflow-y:auto;color:black;border:5px solid gray;background:white;}#vReport pre{white-space:pre-line;}
.half{border:none;}
.ml-2{margin-left:-2px;}
.ba2{border:1px solid gray;}.active{border:1px solid blue;}

.mobile{display:none;}
.desktop{display:block;}
.data th{padding:5px;} .data td {padding: 1px;}.data td a{padding-top:2px;padding-bottom:2px;}

.odd{background: none repeat scroll 0 0 #EFF4FB;}
.floatl{float:left;}
.floatr{float:right;}

.bbr{border-right:1px solid black;}

.current{background:#fff684;}

#header{
padding: 0.1em 0em 0.3em 0.3em;
background-color:#333333;
color:#FFFFFF;
font-family: Verdana;
