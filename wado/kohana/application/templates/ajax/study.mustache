{{#items}}
<li class="txtC bk1 black rel drag normal">
<a class="item block txtC {{class}}" href="{{& href}}">
	<img class="{{image_class}}" src="{{& src}}" />
</a>
{{#is_attachment}}{{text}}<br />
<i>{{type}}</i>{{/is_attachment}}
{{#is_dicom}}
<div class="abs full h100 layer2 none clickPanes pt0 pl0">
	<div class="abs pt0 pl0 hhalf size1of2 txtC bk2 opPane aPane panevTL lhhalf">
		<a href="#vTL" class="border5 bk1 op80 paneLoader black center bfont prl pll">1</a>
	</div>
	<div class="abs pt0 pr0 hhalf size1of2 txtC bk2 opPane aPane panevTR lhhalf">
		<a href="#vTR" class="border5 bk1 op80 paneLoader black center bfont prl pll">2</a>
	</div>
	<div class="abs pb0 pl0 hhalf size1of2 txtC bk2 opPane aPane panevBL lhhalf">
		<a href="#vBL" class="border5 bk1 op80 paneLoader black center bfont prl pll">3</a>
	</div>
	<div class="abs pb0 pr0 hhalf size1of2 txtC bk2 opPane aPane panevBR lhhalf">
		<a href="#vBR" class="border5 bk1 op80 paneLoader black center bfont prl pll">4</a>
	</div>
</div>
<span class="abs pt0 pl0 layer2 black bk3 pls prs fs">{{images}} Img{{^has_1_image}}s{{/has_1_image}}</span>
<a class="abs pb0 pl0 pam plus layer3 none" href="#">&nbsp;</a>
<span class="abs pb0 pl0 full txtC layer2 white bk2 description"><em>{{description}}</em></span>
<span class="abs pt0 pr0 layer2 bk3 pls prs fs">
{{#has_bookmarks}}<a href="{{& bookmarks_href}}">{{/has_bookmarks}}
<em>{{bookmarks}} Bm{{^has_1_bookmark}}s{{/has_1_bookmark}}</em>
{{#has_bookmarks}}</a>{{/has_bookmarks}}
</span>
{{/is_dicom}}
</li>
{{/items}}